        <IRI>#Active</IRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">An Active Resource posseses the ability of transforming a logistics entity by itself.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Active</IRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Active</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Capability</IRI>
        <Literal xml:lang="en" datatypeIRI="&rdf;PlainLiteral">Presents the Capabilities of something.</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>#Capability</IRI>
